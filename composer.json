{
    "name": "snipershady/ip2location",
    "description": "Free wrapper to easily implements ip2location.com free API",
    "keywords": ["ip2location", "geolocation", "ip2location.com"],
    "homepage": "https://www.spinfo.it",
    "type": "library",
    "license": "GPL-3.0-or-later",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Stefano Perrini",
            "email": "perrini.stefano@gmail.com",
            "homepage": "https://www.spinfo.it"
        }
    ],
    "autoload": {
        "psr-4": {
            "IpToLocation\\": "src/"
        }       
    },
    "autoload-dev": {
        "psr-4": {
            "IpToLocation\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=8.2",
        "ext-json": "*",
        "ext-curl": "*",
        "snipershady/typeidentifier": "^1.0.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6.20",
        "phpstan/phpstan": "^1.12.3"
    }
}
